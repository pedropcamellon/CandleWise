{
  "version": 2,
  "name": "candlewise-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://app-candlewise-backend-dev.azurewebsites.net"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "https://app-candlewise-backend-dev.azurewebsites.net"
    }
  }
}